							</groupbox>																		   											                                                                                                                                                                      				    
			   		  </tabpanel>		 
			
			
					<tabpanel orient="vertical">
						
						<groupbox>
						   <caption label="Confirmations"/>    
		
						   <checkbox id="id-zotfile-confirmation" label="Ask user to attach new files" preference="pref-zotfile-confirmation"/>
						   <hbox style="margin: 0"  align="center">					
							    <checkbox id="id-zotfile-confirmation_batch_ask" label="Ask user to (batch) rename or move" preference="pref-zotfile-confirmation_batch_ask" oncommand="updatePreferenceWindow('confirm')"/>	
								<textbox id="id-zotfile-confirmation_batch" preference="pref-zotfile-confirmation_batch" width="25"/> 
								<label id="id-zotfile-confirmation_batch-end"  value="or more attachments."/>																				
							</hbox>     					  			                        		
			  	
						</groupbox> 
												 
						<groupbox>
						   <caption label="Annotation Extraction"/>  
						
