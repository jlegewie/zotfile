						   <checkbox id="id-zotfile-pdfExtraction-MenuItem" label="Add menu item to manually extract annotations from pdf attachments" preference="pref-zotfile-pdfExtraction-MenuItem"/>	
						   <checkbox id="id-zotfile-pdfExtraction-NoteFullCite" label="Add full in-text citation to highlighted text snippets in note" preference="pref-zotfile-pdfExtraction-NoteFullCite"/>	
						   <hbox style="margin: 0"  align="center">														   
							   <checkbox id="id-zotfile-pdfExtraction-NoteTruePage" label="Use actual article/book chapter page for highlighted text snippets" preference="pref-zotfile-pdfExtraction-NoteTruePage"/>				    
