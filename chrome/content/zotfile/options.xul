			<tabbox id="zotfile-tabbox">
				<tabs id="zotfile-tabs">
					<tab label="General Settings"/>
					<tab label="On Your Tablet"/>					
					<tab label="Renaming Rules"/>
					<tab label="Advanced Settings"/>
				</tabs>

				<tabpanels id="zotfile-tabpanels">
					<tabpanel orient="vertical">
		   				 <groupbox>
					
							   <label value="ZotFile can add the most recently modified file from the Firefox download or"/>					
							   <label value="another folder as a new attachment to the currently selected Zotero item."/>					
							   <separator/>
							
							    <caption label="Attaching New File(s)"/>                               								
								<radiogroup id="id-zotfile-source_dir_ff" preference="pref-zotfile-source_dir_ff" oncommand="if(document.getElementById('pref-zotfile-source_dir_ff').value) if(document.getElementById('pref-zotfile-source_dir').value=='') Zotero.ZotFile.prefs.setCharPref('source_dir',Zotero.ZotFile.chooseDirectory()); ">
									<radio id="id-zotfile-source_dir_ff-true" label="Use Firefox download folder" value="true" oncommand="updatePreferenceWindow('source');"/>
									<hbox  style="margin: 0"  align="center">									
									 	<radio label="Custom:" value="false"  oncommand="updatePreferenceWindow('source');" />										
										<textbox id="id-zotfile-source_dir" preference="pref-zotfile-source_dir" flex="1" onchange="updateFolderIcon('source',false); "/>
										<button id="id-zotfile-source_dir-button" label="Choose..." oncommand="Zotero.ZotFile.prefs.setCharPref('source_dir',Zotero.ZotFile.chooseDirectory());"/> <!-- updateFolderIcon('source',false);-->
										<image id="id-zotfile-source_dir-clear" width='16' height='16' hidden="false"/>										    
									    <image id="id-zotfile-source_dir-ok" src="chrome://zotfile/skin/accept.png" hidden="true"/>	
									    <image id="id-zotfile-source_dir-error" src="chrome://zotfile/skin/exclamation.png" hidden="true"/>		
									</hbox>
								</radiogroup>	

							</groupbox>
			
							<separator/> 
							<groupbox>
							   <caption label="Location of Files"/>    
			                    <label value="ZotFile can move new and existing attachments to different locations. You can either"/>
			                    <label value="store a copy of your attachment files in Zotero, which allows you to sync files to the"/>
			                    <label value="Zotero server, or move the file to a custom folder and link to that location from Zotero."/>											
								<separator/>

							   	<radiogroup id="id-zotfile-import" preference="pref-zotfile-import" oncommand="if(document.getElementById('pref-zotfile-import').value) if(document.getElementById('pref-zotfile-dest_dir').value=='') Zotero.ZotFile.prefs.setCharPref('dest_dir',Zotero.ZotFile.chooseDirectory());">
									<radio label="Attach stored copy of file(s)" value="true" oncommand="updatePreferenceWindow('dest');"/>
									<hbox  style="margin: 0"  align="center">
										<radio label="Custom Location:" value="false" oncommand="updatePreferenceWindow('dest');"/>		
										<textbox id="id-zotfile-dest_dir" preference="pref-zotfile-dest_dir" flex="1" onchange="updateFolderIcon('dest',false);"/>
										<button id="id-zotfile-dest_dir-button" label="Choose..." oncommand="Zotero.ZotFile.prefs.setCharPref('dest_dir',Zotero.ZotFile.chooseDirectory());"/>		<!--updateFolderIcon('dest',false);-->
										<image id="id-zotfile-dest_dir-clear" width='16' height='16' hidden="false"/>										    
									    <image id="id-zotfile-dest_dir-ok" src="chrome://zotfile/skin/accept.png" hidden="true"/>	
									    <image id="id-zotfile-dest_dir-error" src="chrome://zotfile/skin/exclamation.png" hidden="true"/>		
									</hbox>									   																																									   
								</radiogroup>
								<hbox style="margin: 0"  align="center">									   
								   <checkbox id="id-zotfile-subfolder" label="Use subfolders defined by " preference="pref-zotfile-subfolder"  oncommand="updatePreferenceWindow('subfolder')"/>
								   <textbox id="id-zotfile-subfolderFormat" preference="pref-zotfile-subfolderFormat" width="100"/>
								   <image src="chrome://zotfile/skin/information.png" tooltiptext="Subfolders are based on metadata from Zotero items. Use %a for author name, %y for year, %w for journal/publisher, or %T for item type (see the zotfile website for a full list). Make sure to insert the appropriate separators (/ for Mac OS and other Unix systems and \ for windows)."/>
							   </hbox>
								
							
			                 	  			                        		
							</groupbox>
							<separator/> 
					
						    <hbox style="margin: 0">					
							   <label style="margin-right:0px" value="For a detailed description of each preference see the "/>
							   <label style="margin-left:0px; margin-right:0px" class="text-link" href="http://www.columbia.edu/~jpl2136/zotfile.html" value="zotfile website"/>
							   <label style="margin-left:0px; margin-right:0px" value="."/>								
						    </hbox>
						    <hbox style="margin: 0">					
							   <label style="margin-right:0px" value="If you find this plugin helpful, please consider a "/>
							   <label style="margin-left:0px; margin-right:0px" class="text-link" href="http://www.columbia.edu/~jpl2136/zotfile.html" value="donation"/>
							   <label style="margin-left:0px; margin-right:0px" value="."/>								
						    </hbox>
													 							   		 
